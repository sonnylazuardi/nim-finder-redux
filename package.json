{
  "name": "nim-finder-redux",
  "version": "1.0.0",
  "description": "nim finder using reactjs and redux",
  "main": "index.js",
  "scripts": {
    "prestart": "./node_modules/browserify/bin/cmd.js -t [ babelify --presets [ react es2015 ] ] src/index.js | ./node_modules/uglifyjs/bin/uglifyjs -o dist/index.js",
    "start": "./node_modules/browser-sync/bin/browser-sync.js start --server --files=\"*.js\""
  },
  "keywords": [
    "redux",
    "nim",
    "finder"
  ],
  "author": "sonnylazuardi",
  "license": "MIT",
  "dependencies": {
    "parse": "^1.6.9",
    "react-redux": "^4.0.0",
    "redux": "^3.0.4",
    "redux-thunk": "^1.0.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babelify": "^7.2.0",
    "object-assign": "^4.0.1",
    "react": "^0.14.3",
    "react-addons-pure-render-mixin": "^0.14.3",
    "react-addons-update": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-pure-render": "^1.0.2"
  },
  "devDependencies": {
    "browser-sync": "^2.10.0",
    "browserify": "^12.0.1",
    "uglifyjs": "^2.4.10"
  }
}
